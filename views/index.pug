extends layout

block content
  .wrapper
    h1(data-userId= user._id) #{user.username} welcome to DasGroup
  .title
    h1 User: #{user.username}
  .groupList
    ul.list-group
      each group in groups
        li.list-group-item 
          a(href=resourceBasePath + 'group/' + group.key) #{group.name}
