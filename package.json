{
  "name": "avsp-express-broiler",
  "version": "1.0.0",
  "description": "A simple express broiler plate w/ webpack",
  "main": "app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev node node_modules/nodemon/bin/nodemon.js  bin/www",
    "staging": "cross-env NODE_ENV=staging node bin/www",
    "start": "cross-env NODE_ENV=production node bin/www",
    "build": "webpack --env.NODE_ENV=production",
    "watch": "webpack --watch --env.NODE_ENV=dev"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "logs/*",
      "src/*"
    ]
  },
  "author": "avsphere",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "jsonfile": "^5.0.0",
    "nodemon": "^1.18.4",
    "style-loader": "^0.22.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "config": "^2.0.1",
    "connect-redis": "^3.4.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "github-webhook-handler": "^0.7.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.2.14",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.8",
    "npm": "^6.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "popper.js": "^1.14.4",
    "pug": "^2.0.3",
    "redis": "^2.8.0",
    "serve-favicon": "^2.5.0",
    "shortid": "^2.2.13",
    "winston": "^3.1.0"
  }
}
